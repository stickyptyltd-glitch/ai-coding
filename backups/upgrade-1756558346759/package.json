{
  "name": "ai-coding-agent",
  "version": "2.0.0",
  "description": "An AI-powered coding agent that can analyze, modify, and generate code",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "ai-agent": "./src/cli.js"
  },
  "scripts": {
    "start": "node src/web-server.js",
    "dev": "node --watch src/web-server.js",
    "web": "node src/web-server.js",
    "web-dev": "nodemon src/web-server.js",
    "test": "node --test test/*.test.js",
    "lint": "eslint src/",
    "security:manifest": "node scripts/gen-manifest.js",
    "security:keys": "node scripts/generate-keys.js",
    "security:sign": "node scripts/sign-license.js",
    "security:rotate": "node scripts/rotate-license.js --days=7",
    "docs:adr:new": "node scripts/new-adr.js",
    "actions:list": "node scripts/gh-actions.js list",
    "actions:watch": "node scripts/gh-actions.js watch",
    "actions:dispatch:rotate": "node scripts/gh-dispatch.js",
    "health:check": "node scripts/health-check.js --url=http://localhost:3000/healthz/strict",
    "gui": "node scripts/open-gui.js",
    "repo:setup": "node scripts/repo-setup.js",
    "upgrade": "node scripts/system-upgrade.js",
    "validate": "node scripts/comprehensive-error-check.js",
    "benchmark": "node scripts/performance-benchmark.js",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "clean": "rm -rf dist/ node_modules/.cache/",
    "release:version:patch": "npm version patch -m 'chore(release): %s'",
    "release:version:minor": "npm version minor -m 'chore(release): %s'",
    "release:version:major": "npm version major -m 'chore(release): %s'",
    "release:tag": "git tag v$npm_package_version && git push origin v$npm_package_version"
  },
  "keywords": [
    "ai",
    "coding",
    "agent",
    "automation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google-cloud/speech": "^7.2.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "axios": "^1.7.9",
    "compression": "^1.7.4",
    "canvas": "^3.2.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dompurify": "^3.2.3",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "face-api.js": "^0.22.2",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.3",
    "inquirer": "^12.9.4",
    "jsdom": "^26.1.0",
    "marked": "^16.2.1",
    "mic": "^2.1.2",
    "node-cron": "^4.2.1",
    "node-record-lpcm16": "^1.0.1",
    "openai": "^5.16.0",
    "puppeteer": "^24.17.1",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "helmet": "^8.1.0",
    "ws": "^8.18.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.5",
    "eslint": "^9.18.0",
    "nodemon": "^3.1.9",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18 <21"
  },
  "overrides": {
    "tar-fs": "^3.0.9",
    "tmp": "^0.2.4",
    "node-fetch": "^2.6.9",
    "node-forge": "^1.3.1"
  }
}
