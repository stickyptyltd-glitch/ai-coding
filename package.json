{
  "name": "ai-coding-agent",
  "version": "1.1.0",
  "description": "An AI-powered coding agent that can analyze, modify, and generate code",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "ai-agent": "./src/cli.js"
  },
  "scripts": {
    "start": "node src/web-server.js",
    "dev": "node --watch src/web-server.js",
    "web": "node src/web-server.js",
    "web-dev": "nodemon src/web-server.js",
    "test": "node --test test/agent.test.js test/web-scraper.test.js",
    "lint": "eslint src/",
    "gui": "node scripts/open-gui.js",
    "repo:setup": "node scripts/repo-setup.js",
    "release:version:patch": "npm version patch -m 'chore(release): %s'",
    "release:version:minor": "npm version minor -m 'chore(release): %s'",
    "release:version:major": "npm version major -m 'chore(release): %s'",
    "release:tag": "git tag v$npm_package_version && git push origin v$npm_package_version"
  },
  "keywords": [
    "ai",
    "coding",
    "agent",
    "automation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@google-cloud/speech": "^7.2.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "axios": "^1.6.2",
    "canvas": "^2.11.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dompurify": "^3.0.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "face-api.js": "^0.22.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "jsdom": "^23.0.1",
    "marked": "^9.1.6",
    "mic": "^2.1.2",
    "node-cron": "^3.0.3",
    "node-record-lpcm16": "^1.0.1",
    "openai": "^4.20.1",
    "puppeteer": "^21.5.2",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18 <21"
  },
  "overrides": {
    "tar-fs": "^3.0.9",
    "ws": "^8.17.1",
    "tmp": "^0.2.4",
    "node-fetch": "^2.6.9",
    "node-forge": "^1.3.1"
  }
}
